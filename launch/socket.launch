<launch>

<node pkg="ROS_tutorials" type="pub_DroneState" name="pub_DS1"  output="screen" launch-prefix="gnome-terminal --">
		<param name="uav_name" value="uav1"/>
</node>

<node pkg="ROS_tutorials" type="pub_DroneState" name="pub_DS2"  output="screen" launch-prefix="gnome-terminal --tab --">
		<param name="uav_name" value="uav2"/>
</node>

<node pkg="ROS_tutorials" type="pub_socket" name="pub_socket" output="screen" launch-prefix="gnome-terminal --tab --">	
		<param name="swarm_num" value="2"/>
		<param name="uav1_name" value="uav1"/>
		<param name="uav2_name" value="uav2"/>
</node>



</launch>